<SETTINGS>
	<OPTIONS>
		<WordDelimeters>" \t\n,./?LTGT;:\"'{[}]~`%^AMP*()-+=!"</WordDelimeters>
		<SpecialDelimiters>";.{}\n"</SpecialDelimiters>
		<IntelliSenseChars>".>"</IntelliSenseChars>
	</OPTIONS>
	<COLOR_DATA>
		<KEYWORDS>
			<Color>0000ff</Color>
			<CaseSensitive>True</CaseSensitive>
			<Keyword>#include</Keyword>
			<Keyword>library</Keyword>
			<Keyword>void</Keyword>
			<Keyword>float</Keyword>
			<Keyword>char</Keyword>
			<Keyword>int</Keyword>
			<Keyword>bool</Keyword>
			<Keyword>string</Keyword>
			<Keyword>struct</Keyword>
			<Keyword>pointer</Keyword>
			<Keyword>object</Keyword>
			<Keyword>interface</Keyword>
			<Keyword>virtual</Keyword>
			<Keyword>handler</Keyword>
			<Keyword>this</Keyword>
			<Keyword>base</Keyword>
			<Keyword>const</Keyword>
			<Keyword>enum</Keyword>
			<Keyword>true</Keyword>
			<Keyword>false</Keyword>
			<Keyword>null</Keyword>
			<Keyword>new</Keyword>
			<Keyword>delete</Keyword>
			<Keyword>sizeof</Keyword>
			<Keyword>typeof</Keyword>
			<Keyword>if</Keyword>
			<Keyword>else</Keyword>
			<Keyword>while</Keyword>
			<Keyword>do</Keyword>
			<Keyword>for</Keyword>
			<Keyword>break</Keyword>
			<Keyword>continue</Keyword>
			<Keyword>return</Keyword>
			<Keyword>switch</Keyword>
			<Keyword>case</Keyword>
			<Keyword>default</Keyword>
			<Keyword>funcptr</Keyword>
			<Keyword>library</Keyword>
			<Keyword>interface</Keyword>
			<Keyword>virtual</Keyword>
			<Keyword>assert</Keyword>
			<Keyword>debug</Keyword>
			<Keyword>debuglog</Keyword>
		</KEYWORDS>
		<WORDS>
			<WORD>
				<Val>#include</Val>
				<Color>"darkred"</Color>
			</WORD>
			<WORD>
				<Val>#define</Val>
				<Color>"darkred"</Color>
			</WORD>
			<WORD>
				<Val>#undef</Val>
				<Color>"darkred"</Color>
			</WORD>
			<WORD>
				<Val>#if</Val>
				<Color>"darkred"</Color>
			</WORD>
			<WORD>
				<Val>#else</Val>
				<Color>"darkred"</Color>
			</WORD>
			<WORD>
				<Val>#endif</Val>
				<Color>"darkred"</Color>
			</WORD>
		</WORDS>
		<BLOCKS>
			<BLOCK>
				<Start>/*</Start>
				<End>*/</End>
				<Color>"darkgreen"</Color>
				<WholeText>true</WholeText>
			</BLOCK>
			<BLOCK>
				<Start>"</Start>
				<End>"</End>
				<Color>"darkblue"</Color>
				<WholeText>false</WholeText>
			</BLOCK>
			<BLOCK>
				<Start>'</Start>
				<End>'</End>
				<Color>"darkblue"</Color>
				<WholeText>false</WholeText>
			</BLOCK>
			<BLOCK>
				<Start>//</Start>
				<Color>"darkgreen"</Color>
				<WholeText>false</WholeText>
			</BLOCK>
		</BLOCKS>
	</COLOR_DATA>
</SETTINGS>